{
  "name": "rbui",
  "version": "0.0.12",
  "private": true,
  "packageManager": "yarn@3.2.1",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "start": "webpack serve --open",
    "build": "NODE_ENV=production webpack --mode production",
    "test": "npm-run-all 'test:*'",
    "wdio": "wdio run wdio.conf.ts",
    "allure": "allure serve wdio/reports/allure-reports",
    "synth": "cdk synth",
    "test:lint": "npm-run-all 'lint:*'",
    "test:code": "jest",
    "test:build-storybook": "yarn run build-storybook",
    "test:build": "yarn run build",
    "test:dist": "yarn run dist",
    "test:synth": "yarn run synth",
    "lint:ts": "tsc --noemit",
    "lint:code": "eslint . --ext .js,.jsx,.ts,.tsx",
    "build-storybook": "NODE_ENV=production build-storybook --quiet -o storybook",
    "postinstall": "husky install",
    "prebuild": "rimraf build",
    "pretest": "rimraf coverage",
    "prebuild-storybook": "rimraf storybook",
    "prewdio": "rimraf wdio/reports",
    "predist": "rimraf dist",
    "publish": "sh bin/publish",
    "dist": "sh bin/dist",
    "drawio:RBUIProductionCdkStack": "cfn-dia draw.io -c -t cdk.out/RBUIProductionCdkStack.template.json -o cdk.out/RBUIProductionCdkStack.drawio",
    "drawio:RBUIStagingCdkStack": "cfn-dia draw.io -c -t cdk.out/RBUIStagingCdkStack.template.json -o cdk.out/RBUIStagingCdkStack.drawio",
    "drawio:RBUITestCdkStack": "cfn-dia draw.io -c -t cdk.out/RBUITestCdkStack.template.json -o cdk.out/RBUITestCdkStack.drawio",
    "jsdoc": "jsdoc dist -r -d jsdoc",
    "presynth": "rimraf cdk.out",
    "postsynth": "npm-run-all 'drawio:*'",
    "deploy:rbui": "cdk deploy RBUICdkStack",
    "deploy:production": "cdk deploy RBUIProductionCdkStack",
    "deploy:staging": "cdk deploy RBUIStagingCdkStack",
    "deploy:test": "cdk deploy RBUITestCdkStack",
    "postdeploy:production": "STAGE=production ts-node bin/upload.ts",
    "postdeploy:staging": "STAGE=staging ts-node bin/upload.ts",
    "postdeploy:test": "STAGE=test ts-node bin/upload.ts"
  },
  "devDependencies": {
    "@4c/tsconfig": "^0.4.1",
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/polyfill": "^7.12.1",
