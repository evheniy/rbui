{
  "name": "rbui",
  "version": "0.0.9",
  "private": true,
  "packageManager": "yarn@3.1.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "start": "webpack serve --open",
    "build": "NODE_ENV=production webpack --mode production",
    "test": "npm-run-all 'test:*'",
    "wdio": "wdio run wdio.conf.ts",
    "allure": "allure serve wdio/reports/allure-reports",
    "test:lint": "npm-run-all 'lint:*'",
    "test:code": "jest",
    "test:build-storybook": "yarn run build-storybook",
    "test:build": "yarn run build",
    "lint:ts": "tsc --noemit",
    "lint:code": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint1:style": "stylelint 'packages/**/*.{scss,css}'",
    "build-storybook": "NODE_ENV=production build-storybook --quiet",
    "postinstall": "husky install",
    "prebuild": "rimraf build",
    "pretest": "rimraf coverage",
    "prebuild-storybook": "rimraf storybook-static",
    "prewdio": "rimraf wdio/reports",
    "predist": "rimraf dist",
    "postdist": "sh bin/copy",
    "publish": "sh bin/publish",
    "dist": "sh bin/dist"
  },
